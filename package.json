{
  "name": "arcite-webclient",
  "version": "1.0.0",
  "description": "Web client for ARCITE",
  "main": "index.js",
  "repository": "",
  "scripts": {
    "init": "npm run clean && mkdir -p dist/public",
    "start": "npm run build && node ./dist/server.js",    
    "build": "npm run init && cp ./src/index.html ./dist/public/ && npm run build-client && npm run build-server",
    "watch": "npm run init && cp ./src/index.html ./dist/public/ && npm run build-server && npm run watch-client",
    "build-client": "export NODE_ENV=production; webpack --progress --colors --config ./webpack.prod.config.js",
    "watch-client": "export NODE_ENV=production; webpack --progress --colors --watch --config ./webpack.prod.config.js",
    "build-server": "export NODE_ENV=production; node ./node_modules/babel-cli/bin/babel.js -o dist/server.js src/server/server.js",
    "lint": "eslint ./src/ --ext .js --ext .jsx --cache; exit 0",
    "clean": "rm -rf ./dist || TRUE",
    "test": "jest; exit 0",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "bamboo-lint": "eslint ./src/ --ext .js --ext .jsx -f ./node_modules/eslint-bamboo-formatter/reporter.js; exit 0",
    "bamboo-test": "jest --config=./config/jest-bamboo.config.json; exit 0"
  },
  "author": "Tobias Fink <tobias.fink@idorsia.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-bamboo-formatter": "^0.0.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.5.0",
    "file-loader": "^0.9.0",
    "jest-bamboo-formatter": "^0.0.3",
    "jest": "^21.1.0",
    "jest-cli": "^21.1.0",    
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.11.1",
    "postcss-loader": "^1.1.0",
    "postcss-normalize": "^0.3.0",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "@kadira/storybook": "^2.32.1",
    "all": "^0.0.0",
    "axios": "^0.15.2",
    "axios-mock-adapter": "^1.9.0",
    "blueimp-md5": "^2.6.0",
    "chroma-js": "^1.2.1",
    "classnames": "^2.2.5",
    "d3": "^4.4.0",
    "distinct-colors": "^1.0.4",
    "elasticlunr": "^0.9.5",
    "express": "^4.14.0",
    "http-proxy-middleware": "^0.18.0",
    "jsbarcode": "^3.5.5",
    "lodash": "^4.16.6",
    "material-ui": "^0.16.4",
    "moment": "^2.17.0",
    "node-uuid": "^1.4.7",
    "papaparse": "^4.1.2",
    "prop-types": "^15.5.10",
    "radium": "^0.18.1",
    "react": "^15.3.2",
    "react-avatar": "^2.2.1",
    "react-dimensions": "^1.3.0",
    "react-dom": "^15.3.2",
    "react-motion": "^0.4.7",
    "react-redux": "^5.0.6",
    "react-retina-image": "^2.0.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-spinkit": "^2.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-form": "^6.1.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^2.5.4"
  }
}
